<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/widget45"
				android:layout_width="fill_parent" android:layout_height="fill_parent"
				xmlns:android="http://schemas.android.com/apk/res/android">
				<TableLayout android:id="@+id/ResourceTableLayout" android:layout_height="wrap_content" android:layout_width="fill_parent">
				<TableRow android:gravity="center" android:id="@+id/HeaderTableRow" >
					<TextView android:background="@drawable/location_left"></TextView>
					<TextView android:id="@+id/locationtext1" android:text="UNKNOWN: " android:textStyle="bold" android:gravity="center_vertical" android:background="@drawable/location2" android:layout_height="wrap_content" android:textColor="#ffffff"  android:textSize="20dp" android:layout_width="wrap_content" 
					android:layout_marginRight="0dip">
					</TextView>
					<TextView android:id="@+id/locationtext2" android:text="Unknown" android:layout_marginLeft="0dip" android:paddingTop="5dip" android:background="@drawable/location2" android:textColor="#ffffff" android:gravity="center_vertical" android:layout_height="wrap_content" 
					 android:layout_weight="1">
					</TextView>
				</TableRow>
				</TableLayout>
					<ListView android:id="@+id/resourcelist"
						android:layout_width="fill_parent" android:layout_height="fill_parent"
						android:layout_below="@+id/ResourceTableLayout"
						android:layout_alignParentLeft="true">
					</ListView>			
</RelativeLayout>
